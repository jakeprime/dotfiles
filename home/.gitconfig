# This is Git's per-user configuration file.
[core]
# Please adapt and uncomment the following lines:
	user = jake
	email = jake@jakeprime.com
	excludesfile = ~/.gitignore_global
	editor = vim
  pager = delta
[push]
	default = simple
[user]
	name = Jake Prime
	email = jake@jakeprime.com
[filter "lfs"]
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
[credential]
	helper = osxkeychain

[alias]
	set-upstream = !git branch --set-upstream-to=origin/`git symbolic-ref --short HEAD`
	pushf = push --force-with-lease
	praise = blame
	cleanup = !git fetch --prune && git branch -vv | awk \"/: gone]/{print \\$1}\" | xargs git branch -d
	cleanupf = !git fetch --prune && git branch -vv | awk \"/: gone]/{print \\$1}\" | xargs git branch -D

[init]
	defaultBranch = main
[branch]
	autosetuprebase = always
[rebase]
	autosquash = true
	autoRebase = true
[commit]
	verbose = true
[merge]
	conflictstyle = diff3
[github]
	user = jakeprime
[interactive]
  diffFilter = delta --color-only
[delta]
  light = false
  max-line-length = 0
  minus-emph-style = "black red"
  minus-style = "#red normal"
  minus-style = "red normal"
  navigate = true
  plus-emph-style = "black green"
  plus-style = "green normal"
  zero-style = "brightblack normal"
[diff]
  colorMoved = default
[filter "readline-gemfile-gitignore"]
	clean = sed '/readline-ext/d'
	smudge = cat
